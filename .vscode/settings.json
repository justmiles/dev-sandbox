{
  "workbench.colorCustomizations": {
    "activityBar.activeBorder": "#8a6996",
    "activityBar.activeBackground": "#992500",
    "activityBar.background": "#992500",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#002008",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#992500",
    "statusBar.background": "#661900",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#992500",
    "statusBarItem.remoteBackground": "#661900",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#661900",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#66190099",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "customLocalFormatters.formatters": [
    {
      "command": "~/.nix-profile/bin/hclfmt",
      "languages": [
        "hcl"
      ]
    },
    {
      "command": "~/.local/bin/terraform fmt -",
      "languages": ["terraform"]
    },
    {
      "command": "~/.nix-profile/bin/nomad fmt -",
      "languages": ["hcl"]
    }
  ],
  "files.associations": {
    "*.hcl": "hcl",
    "*.nomad.hcl": "hcl",
    "*.pkr.hcl": "hcl"
  },
  "[terraform]": {
    "editor.defaultFormatter": "jkillian.custom-local-formatters"
  },
  "[packer]": {
    "editor.defaultFormatter": "jkillian.custom-local-formatters"
  },
  "[nomad]": {
    "editor.defaultFormatter": "jkillian.custom-local-formatters"
  },
  "peacock.remoteColor": "#661900"
}
